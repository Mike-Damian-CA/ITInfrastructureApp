<%= simple_form_for(@user) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
  </div>

  <div class="form-actions">
    <%= f.input :name %>
    <%= f.input :dob, start_year: Date.today.year - 110, end_year: Date.today.year %>
    <%= f.input :phone %>
    <%= f.input :mobile %>
    <%= f.input :email %>
    <%= f.button :submit %>
    <p></p>
  </div>
<% end %>
