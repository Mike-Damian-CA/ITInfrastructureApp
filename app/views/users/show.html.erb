<!--p id="notice"><%= notice %></p-->
<div class="container-fluid">
  <h1>Show User</h1>
  <div class="row col-xs-6 col-sm-6 col-md-6 col-lg-6">
   <table class="table table-striped table-bordered table-hover table-condensed table-responsive">
     <tbody>
       <tr>
         <td style="background-color: #dddddd"><strong>ID</strong></td>
         <td style="background-color: #dddddd"><%= @user.id %></td>
         </tr>
         <tr>
           <td><strong>Created At</strong></td><td><%= @user.created_at %></td>
         </tr>
         <tr>
           <td><strong>Updated At</strong></td><td><%= @user.updated_at %></td>
         </tr>
         <tr>
           <td><strong>Name</strong></td><td><%= @user.name %></td>
         </tr>
         <tr>
           <td><strong>Date of Birth</strong></td><td><%= @user.dob %></td>
         </tr>
         <tr>
           <td><strong>Phone Number</strong></td><td><%= @user.phone %></td>
         </tr>
         <tr>
           <td><strong>Mobile Number</strong></td><td><%= @user.mobile %></td>
         </tr>
         <tr>
           <td><strong>Email</strong></td><td><%= @user.email %></td>
         </tr>
       </tbody>
     </table>
    <%= link_to 'Edit', edit_user_path(@user), class: "btn btn-md btn-danger" %>
    <%= link_to 'Back', users_path, class: "btn btn-md btn-danger" %>
    <%= link_to 'Delete', @user, class: "btn btn-md btn-danger", method: :delete, data: { confirm: 'Are you sure?' } %>
  </div>

</div>
