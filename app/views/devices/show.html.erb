<div class="container-fluid">
  <h1>Show Device</h1>
  <div class="row col-xs-6 col-sm-6 col-md-6 col-lg-6">
   <table class="table table-striped table-bordered table-hover table-condensed table-responsive">
     <tbody>
       <tr>
         <td style="background-color: #dddddd"><strong>ID</strong></td><td style="background-color: #dddddd"><%= @device.id %></td>
       </tr>
       <tr>
         <td><strong>Created At</strong></td><td><%= @device.created_at %></td>
       </tr>
       <tr>
         <td><strong>Updated Ate</strong></td><td><%= @device.updated_at %></td>
       </tr>
       <tr>
         <td><strong>Name</strong></td><td><%= @device.name %></td>
       </tr>
       <tr>
         <td><strong>Locatione</strong></td><td><%= @device.location %></td>
       </tr>
     </tbody>
   </table>
   <%= link_to 'Edit', edit_device_path(@device), class: "btn btn-md btn-danger" %>
   <%= link_to 'Back', devices_path, class: "btn btn-md btn-danger" %>
 </div>

</div>
